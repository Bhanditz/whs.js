{
    "env": {
        "node": {
            "presets": [
                "es2015"
            ],
            "plugins": [
                ["transform-runtime", { "polyfill": false }],
                "add-module-exports",
                "transform-decorators-legacy",
				"transform-class-properties"
            ]
        },
        "browser": {
            "presets": [
                "es2015-rollup"
            ]
        }
    }
}
